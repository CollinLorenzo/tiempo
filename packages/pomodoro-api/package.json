{
  "name": "pomodoro-api",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "9.3.0",
    "yarn": "1.3.2"
  },
  "scripts": {
    "start": "nodemon ./index",
    "serve": "node ./index",
    "test": "jest",
    "db:setup": "yarn db:create && yarn db:migrate && yarn db:seed",
    "db:redo": "yarn db:drop && yarn db:setup",
    "db:drop": "sequelize db:drop",
    "db:create": "sequelize db:create",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "db:model:generate": "sequelize model:generate --name",
    "db:migration:generate": "sequelize migration:generate --name",
    "db:seed:generate": "sequelize seed:generate --name"
  },
  "dependencies": {
    "express": "^4.16.2",
    "pg": "6",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.28.6",
    "sequelize-cli": "^3.2.0"
  },
  "devDependencies": {
    "jest": "^22.0.4",
    "nodemon": "^1.14.3"
  }
}
