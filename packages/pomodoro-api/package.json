{
  "name": "pomodoro-api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./index --exec babel-node",
    "sequelize": "babel-node ./node_modules/.bin/sequelize",
    "db:setup": "yarn db:create && yarn db:migrate && yarn db:seed",
    "db:redo": "yarn db:drop && yarn db:setup",
    "db:drop": "yarn sequelize db:drop",
    "db:create": "yarn sequelize db:create",
    "db:migrate": "yarn sequelize db:migrate",
    "db:seed": "yarn sequelize db:seed:all",
    "db:model:generate": "yarn sequelize model:generate --name",
    "db:migration:generate": "yarn sequelize migration:generate --name",
    "db:seed:generate": "yarn sequelize seed:generate --name"
  },
  "dependencies": {
    "express": "^4.16.2",
    "pg": "6",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.28.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "nodemon": "^1.14.3",
    "sequelize-cli": "^3.2.0"
  }
}
