{
  "name": "pomodoro-api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./index",
    "test": "jest",
    "deploy": "now --public && now alias",
    "prod": "node ./index",
    "now-build": "yarn db:migrate && yarn db:seed",
    "now-start": "yarn prod",
    "db:redo": "yarn db:drop && yarn db:setup",
    "db:setup": "yarn db:create && yarn db:migrate && yarn db:seed",
    "db:drop": "sequelize db:drop",
    "db:create": "sequelize db:create",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "db:model:create": "sequelize model:generate --name",
    "db:migration:create": "sequelize migration:generate --name",
    "db:seed:create": "sequelize seed:generate --name"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "morgan": "^1.9.0",
    "pg": "6",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.28.6",
    "sequelize-cli": "^3.2.0",
    "socket.io": "^2.0.4"
  },
  "devDependencies": {
    "jest": "^22.0.4",
    "nodemon": "^1.14.3",
    "now": "^9.0.1"
  }
}
